<table class="table table-bordered border-primary table-hover">
  <% @results.each do |row| %>
    <tr>
      <td><%= row['id'] %></td>
      <td><%= row['username'] %></td>
      <td><%= row['phone'] %></td>
      <td><%= row['datestamp'] %></td>
      <td><%= row['persons'] %></td>
      <td><%= row['color'] %></td>
    </tr>
  <% end %>

</table>